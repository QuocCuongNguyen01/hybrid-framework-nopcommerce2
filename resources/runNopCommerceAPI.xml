<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="none" name="Nopcommerce - Web Automation API ">

<listeners>
		<listener class-name="commons.MethodListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Payment test suite">
		<parameter name="browser" value="firefox" />
		<classes>

			<class name="com.nopcommerce.account.Level_15_Assert_Verify" />

		</classes>
	</test>
	<test name="User test suite">
		<parameter name="browser" value="chrome" />
		<classes>

			<class name="com.nopcommerce.account.Level_15_Assert_Verify" />

		</classes>
	</test>
	<test name="Run on Edge">
		<parameter name="browser" value="edge" />
		<classes>

			<class name="com.nopcommerce.account.Level_16_Log" />

		</classes>
	</test>

